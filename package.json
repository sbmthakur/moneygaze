{
  "name": "moneygaze",
  "version": "0.0.1",
  "description": "Get all accounts in one place, track subscriptions etc.",
  "private": true,
  "main": "server.js",
  "scripts": {
    "build-ts": "tsc -p ./code/tsconfig.json",
    "build": "npm run build-ts",
    "build-client": "next build",
    "generate": "npx prisma generate",
    "next-dev": "next",
    "clean": "rimraf dist/",
    "coverage": "nyc --reporter=lcov --reporter=text-summary npm test",
    "dev": "concurrently --kill-others --prefix \"[{name}]\" --names \"Typescript,Node\" -c \"cyan.bold,green.bold\" \"npm run watch-ts\" \"wait-on dist/server.js && npm run serve-debug\"",
    "format-schema": "npx prisma format",
    "migrate-dev": "npx prisma migrate dev --name init",
    "test": "mocha --require ts-node/register 'code/test.ts'",
    "serve": "node dist/server.js",
    "serve-debug": "node --inspect dist/server.js",
    "start": "next start",
    "webdev": "npm run build && run-p next-dev serve",
    "watch-ts": "tsc -w -p ./code/tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sbmthakur/moneygaze.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/sbmthakur/moneygaze/issues"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.15",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.11.18",
    "@types/proxyquire": "^1.3.28",
    "@types/react": "18.0.28",
    "@types/sinon": "^10.0.13",
    "concurrently": "^7.6.0",
    "mocha": "^10.2.0",
    "npm-run-all": "^4.1.5",
    "nyc": "^15.1.0",
    "prisma": "^4.9.0",
    "proxyquire": "^2.1.3",
    "rimraf": "^4.1.1",
    "run-p": "^0.0.0",
    "sinon": "^15.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4",
    "wait-on": "^7.0.1"
  },
  "dependencies": {
    "@emotion/cache": "^11.10.5",
    "@emotion/react": "^11.10.6",
    "@emotion/server": "^11.10.0",
    "@emotion/styled": "^11.10.6",
    "@mui/icons-material": "^5.11.9",
    "@mui/material": "^5.11.10",
    "@mui/x-data-grid": "^5.17.24",
    "@next/font": "^13.1.6",
    "@prisma/client": "^4.9.0",
    "@tanstack/react-query": "^4.24.10",
    "@tanstack/react-query-devtools": "^4.24.10",
    "apexcharts": "^3.37.1",
    "axios": "^1.3.4",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "iron-session": "^6.3.1",
    "js-cookie": "^3.0.1",
    "jsonwebtoken": "^9.0.0",
    "moment": "^2.29.4",
    "nanoid": "^4.0.1",
    "next": "^13.1.6",
    "plaid": "^12.3.0",
    "react": "^18.2.0",
    "react-apexcharts": "^1.4.0",
    "react-dom": "^18.2.0",
    "react-plaid-link": "^3.3.2",
    "react-pro-sidebar": "^1.0.0",
    "react-scripts": "5.0.1",
    "simple-zustand-devtools": "^1.1.0",
    "zustand": "^4.3.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "proxy": "http://localhost:3000"
}
